{
  "compilerOptions": {
    "module": "NodeNext",
    "lib": ["ESNext"],
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": true,
    "emitDeclarationOnly": false,
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "erasableSyntaxOnly": true,
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true
  }
}
